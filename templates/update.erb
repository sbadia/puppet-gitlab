#!<%= @git_home %>/.rbenv/shims/ruby

# This file was placed here by GitLab. It makes sure that your pushed commits
# will be processed properly.
# You can add your own hooks to this file, but be careful when updating gitlab-shell!

refname = ARGV[0]
key_id  = ENV['GL_ID']
repo_path = Dir.pwd

update_file = '<%= @git_home %>/gitlab-shell/lib/gitlab_update.rb'
if File.file?(update_file)
        require update_file
        GitlabUpdate.new(repo_path, key_id, refname).exec
end
